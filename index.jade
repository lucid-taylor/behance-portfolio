html(ng-app="portfolio" ng-cloak)
	head
		title Design by Taylor Palmer
		meta(name="description" content="I'm a designer and coder in Utah. This is a hub for all of my design work and social channels.")
		meta(name="ROBOTS" content="INDEX, FOLLOW")
		meta(http-equiv="Content-Type" content="text/html; charset=UTF-8")
		meta(name="viewport" content="width=device-width, initial-scale=1")
		link(href="https://fonts.googleapis.com/css?family=Libre+Franklin:400,600" rel="stylesheet")
		link(href="css/style.css" rel="stylesheet" type="text/css")
	body(ng-controller="portfolioCtrl")
		.wrapper
			header#main-header.row
				.box
				h1
					span.red Hi, I'm Taylor.
					div Iâ€™m a designer and coder in Utah. You can find me designing at 
						a(href="golucid.co" title="Lucid Software") Lucid Software
						| , or you can see my work below.
			.projects.row
				header
					a(href="https://www.behance.net/tpalmerixd" title="Behance")
						include includes/behance.svg
						h2 Behance
						include includes/arrow-right.svg
				ul.flex-4.placeholder(ng-hide="behanceLoaded")
					li
						.image-container
						.text-block
						.text-block.short
					li
						.image-container
						.text-block
						.text-block.short
					li
						.image-container
						.text-block
						.text-block.short
					li
						.image-container
						.text-block
						.text-block.short
				ul.flex-4.behance.ng-hide(ng-show="behanceLoaded")
					li(ng-repeat="project in behance | limitTo:4")
						a(href="{{ project.url }}")
							.image-container
								img(ng-src="{{ project.covers.original }}" alt="{{ project.name }}")
							h3 {{ project.name }}
			.projects.row.half
				header
					include includes/medium.svg
					h2 Medium
					include includes/arrow-right.svg
				ul.medium.ng-hide(ng-show="mediumLoaded")
					li(ng-repeat="story in medium | limitTo:4")
						a(href="{{ story.link }}")
							include includes/bookmark-outline.svg
							h3 {{ story.title }}
			.projects.row
				header
					a(href="https://dribbble.com/taylorpalmer" title="Dribbble")
						include includes/dribbble.svg
						h2 Dribbble
						include includes/arrow-right.svg
				ul.flex-4.placeholder(ng-hide="dribbbleLoaded")
					li
						.image-container
						.text-block
						.text-block.short
					li
						.image-container
						.text-block
						.text-block.short
					li
						.image-container
						.text-block
						.text-block.short
					li
						.image-container
						.text-block
						.text-block.short
					li
						.image-container
						.text-block
						.text-block.short
					li
						.image-container
						.text-block
						.text-block.short
					li
						.image-container
						.text-block
						.text-block.short
					li
						.image-container
						.text-block
						.text-block.short
				ul.flex-4.dribbble.ng-hide(ng-show="dribbbleLoaded")
					li(ng-repeat="project in dribbble | limitTo:8")
						a(href="{{ project.html_url }}")
							.image-container
								img(ng-src="{{ project.images.normal }}" alt="{{ project.title }}")
							h3 {{ project.title }}
                
